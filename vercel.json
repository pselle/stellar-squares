{
  "buildCommand": null,
  "outputDirectory": "dist",
  "framework": null,
  "installCommand": "echo 'Skipping install - using pre-built assets'",
  "builds": [
    {
      "src": "dist/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
